{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenberg/call-to-action",
  "title": "Call to Action",
  "category": "design",
  "icon": "megaphone",
  "description": "Call to action with background image, title, description and button.",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "layout": {
      "type": "string",
      "default": "left"
    },
    "bgImageUrl": {
      "type": "string",
      "default": ""
    },
    "bgImageId": {
      "type": "number",
      "default": 0
    },
    "bgImageSize": {
      "type": "string",
      "default": "full"
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": ""
    },
    "buttonUrl": {
      "type": "string",
      "default": ""
    },
    "buttonTarget": {
      "type": "boolean",
      "default": false
    },
    "buttonNofollow": {
      "type": "boolean",
      "default": false
    },
    "alignment": {
      "type": "string",
      "default": "left"
    },
    "imageMinHeight": {
      "type": "number",
      "default": 425
    },
    "contentBgColor": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": "#000000"
    },
    "titleSize": {
      "type": "number",
      "default": 28
    },
    "titleFontFamily": {
      "type": "string",
      "default": ""
    },
    "titleFontWeight": {
      "type": "string",
      "default": ""
    },
    "titleTextTransform": {
      "type": "string",
      "default": ""
    },
    "titleFontStyle": {
      "type": "string",
      "default": ""
    },
    "titleTextDecoration": {
      "type": "string",
      "default": ""
    },
    "titleLineHeight": {
      "type": "string",
      "default": ""
    },
    "titleLetterSpacing": {
      "type": "string",
      "default": ""
    },
    "titleWordSpacing": {
      "type": "string",
      "default": ""
    },
    "descriptionColor": {
      "type": "string",
      "default": "#666666"
    },
    "descriptionSize": {
      "type": "number",
      "default": 16
    },
    "descriptionFontFamily": {
      "type": "string",
      "default": ""
    },
    "descriptionFontWeight": {
      "type": "string",
      "default": ""
    },
    "descriptionTextTransform": {
      "type": "string",
      "default": ""
    },
    "descriptionFontStyle": {
      "type": "string",
      "default": ""
    },
    "descriptionTextDecoration": {
      "type": "string",
      "default": ""
    },
    "descriptionLineHeight": {
      "type": "string",
      "default": ""
    },
    "descriptionLetterSpacing": {
      "type": "string",
      "default": ""
    },
    "descriptionWordSpacing": {
      "type": "string",
      "default": ""
    },
    "descriptionSpacing": {
      "type": "number",
      "default": 0
    },
    "buttonBgColor": {
      "type": "string",
      "default": "#007cba"
    },
    "buttonTextColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "buttonSize": {
      "type": "number",
      "default": 16
    },
    "buttonFontFamily": {
      "type": "string",
      "default": ""
    },
    "buttonFontWeight": {
      "type": "string",
      "default": ""
    },
    "buttonTextTransform": {
      "type": "string",
      "default": ""
    },
    "buttonFontStyle": {
      "type": "string",
      "default": ""
    },
    "buttonTextDecoration": {
      "type": "string",
      "default": ""
    },
    "buttonLineHeight": {
      "type": "string",
      "default": ""
    },
    "buttonLetterSpacing": {
      "type": "string",
      "default": ""
    },
    "buttonWordSpacing": {
      "type": "string",
      "default": ""
    },
    "buttonBorderRadius": {
      "type": "number",
      "default": 4
    },
    "buttonPadding": {
      "type": "object",
      "default": {
        "top": 12,
        "right": 24,
        "bottom": 12,
        "left": 24
      }
    },
    "contentPadding": {
      "type": "object",
      "default": {
        "top": 50,
        "right": 50,
        "bottom": 50,
        "left": 50
      }
    },
    "contentMargin": {
      "type": "object",
      "default": {
        "top": 0,
        "right": 0,
        "bottom": 0,
        "left": 0
      }
    },
    "ribbonTitle": {
      "type": "string",
      "default": ""
    },
    "ribbonBgColor": {
      "type": "string",
      "default": "#007cba"
    },
    "ribbonTextColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "ribbonSize": {
      "type": "number",
      "default": 16
    },
    "ribbonFontFamily": {
      "type": "string",
      "default": ""
    },
    "ribbonFontWeight": {
      "type": "string",
      "default": ""
    },
    "ribbonTextTransform": {
      "type": "string",
      "default": ""
    },
    "ribbonFontStyle": {
      "type": "string",
      "default": ""
    },
    "ribbonTextDecoration": {
      "type": "string",
      "default": ""
    },
    "ribbonLineHeight": {
      "type": "string",
      "default": ""
    },
    "ribbonLetterSpacing": {
      "type": "string",
      "default": ""
    },
    "ribbonWordSpacing": {
      "type": "string",
      "default": ""
    },
    "ribbonHorizontalPosition": {
      "type": "string",
      "default": "left"
    },
    "ribbonDistance": {
      "type": "number",
      "default": 42
    },
    "className": {
      "type": "string",
      "default": ""
    },
    "anchor": {
      "type": "string",
      "default": ""
    }
  }
}