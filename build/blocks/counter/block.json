{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "progressus/counter",
  "version": "1.0.0",
  "title": "Counter",
  "category": "design",
  "description": "Display a counter with animations",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "startValue": {
      "type": "number",
      "default": 0
    },
    "endValue": {
      "type": "number",
      "default": 100
    },
    "duration": {
      "type": "number",
      "default": 2000
    },
    "prefix": {
      "type": "string",
      "default": ""
    },
    "suffix": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "titleColor": {
      "type": "string",
      "default": ""
    },
    "numberColor": {
      "type": "string",
      "default": ""
    },
    "numberSize": {
      "type": "number",
      "default": 50
    },
    "titleSize": {
      "type": "number",
      "default": 20
    },
    "alignment": {
      "type": "string",
      "default": "center"
    }
  },
  "textdomain": "elementor-to-gutenberg",
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}